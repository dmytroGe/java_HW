package com.gmail.getmandmytro1;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

        int a = 9;
        int b = 5;
        int c = 0;
        
        for (int i = 1; i <= a; i++) {
            c = i * b;
        System.out.println(i + " * " + b + " = " + c);
        }
    }
}
