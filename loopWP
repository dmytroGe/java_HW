package com.gmail.getmandmytro1;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		int wp;
		Scanner sc = new Scanner(System.in);

		System.out.print(" How many lines? ");

		wp = sc.nextInt();

		for (int i = 1; i <= 5; i++) {

			for (int j = 1; j <= wp; j++) {

				if ((j % 2) == 0) {

					System.out.print("***");

				} else {

					System.out.print("+++");
				}
			}
			System.out.println();
		}
	}
}
